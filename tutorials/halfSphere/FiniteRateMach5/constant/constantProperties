/*---------------------------------------------------------------------------*\

\*---------------------------------------------------------------------------*/

FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      solidProperties;
}

// * * * * * *  Units * * * * * [kg m s K mol A cd] * * * * * * * * * * * * * //
// e.g. W: kg m^2 s^{-3}        [1 2 -3 0 0 0 0]
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//Cp
Cp		Cp [0 2 -2 -1 0 0 0]   1004;

// viscosity
mu 		mu [1 -1 -1 0 0 0 0]	1.85e-05;

//Perfect gas constant
R		R [1 2 -2 -1 -1 0 0]      8.314;

// Molar mass air
M		M [1 0 0 0 -1 0 0]        0.028965;

// Prandlt
Pr	Pr [0 0 0 0 0 0 0]				0.72;

// ************************************************************************* //
