FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      N2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.767;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value		    uniform 0.767;
    }

    outlet
    {
        type            fixedValue;
    	value		    uniform 0.767;
    }

    bottom
    {
        type             empty;
    }
    
    obstacle
    {
        type            zeroGradient;
    }


    wedgefront
    {
        type            wedge;
    }
    
    wedgeback
    {
        type            wedge;
    }
}


