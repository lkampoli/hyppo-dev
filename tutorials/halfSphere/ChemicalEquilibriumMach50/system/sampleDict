FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    location        system;
    object          sampleDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

setFormat    raw;

surfaceFormat           raw;

interpolationScheme cell;


fields
(
    p
);

surfaces
(
    obstacle
    {
        type            patch;
        patchName       obstacle;
    }
);



